<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>Complex Set ● Contract | Editable root list item DAL interface</Title>
            <Author>Mármarosi József</Author>
            <Description>Defines the DAL interface of an editable child model.</Description>
            <Shortcut>c8csrii</Shortcut>
        </Header>
        <Snippet>
            <Code Language="CSharp">
                <![CDATA[/// <summary>
    /// Defines the data access functions of the editable $root_name$ set item object.
    /// </summary>
    public interface I$ROOT_MODEL$SetItemDal
    {
        Task InsertAsync($ROOT_MODEL$SetItemDao dao);
        Task UpdateAsync($ROOT_MODEL$SetItemDao dao);
        Task DeleteAsync($ROOT_MODEL$SetItemCriteria criteria);
    }$end$]]>
            </Code>
            <Declarations>
                <Literal>
                    <ID>root_name</ID>
                    <ToolTip>The description of the root model in comments.</ToolTip>
                    <Default>root_name</Default>
                </Literal>
                <Literal>
                    <ID>ROOT_MODEL</ID>
                    <ToolTip>The name of the root model.</ToolTip>
                    <Default>ROOT_MODEL</Default>
                </Literal>
            </Declarations>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
